{
    "name": "data-dragons",
    "version": "1.0.0",
    "description": "GoodData React SDK application",
    "author": "",
    "private": true,
    "type": "module",
    "exports": "./esm/index.js",
    "types": "./esm/index.d.ts",
    "gooddata": {
        "hostname": "https://jav.demo.cloud.gooddata.com",
        "workspaceId": "ws-snowflake-demo",
        "backend": "tiger",
        "catalogOutput": "src/catalog.ts"
    },
    "scripts": {
        "clean": "rm -rf esm *.log",
        "build": "webpack --mode production",
        "start": "webpack-cli serve",
        "refresh-md": "gdc-catalog-export",
        "eslint": "eslint -c .eslintrc.cjs src/",
        "eslint-fix": "eslint -c .eslintrc.cjs --fix src/",
        "fix": "yarn prettier-fix && yarn eslint-fix",
        "prettier-check": "yarn prettier --check .",
        "prettier-fix": "yarn prettier --write .",
        "prepare": "husky install"
    },
    "dependencies": {
        "@chakra-ui/react": "^2.8.1",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@gooddata/sdk-backend-base": "9.2.0",
        "@gooddata/sdk-backend-bear": "9.2.0",
        "@gooddata/sdk-backend-spi": "9.2.0",
        "@gooddata/sdk-backend-tiger": "9.2.0",
        "@gooddata/sdk-model": "9.2.0",
        "@gooddata/sdk-ui": "9.2.0",
        "@gooddata/sdk-ui-charts": "9.2.0",
        "@gooddata/sdk-ui-dashboard": "9.2.0",
        "@gooddata/sdk-ui-ext": "9.2.0",
        "@gooddata/sdk-ui-filters": "9.2.0",
        "@gooddata/sdk-ui-geo": "9.2.0",
        "@gooddata/sdk-ui-pivot": "9.2.0",
        "framer-motion": "^10.16.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.47.0",
        "react-router-dom": "^6.17.0",
        "tslib": "^2.5.0",
        "zustand": "^4.4.3"
    },
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/preset-env": "^7.7.2",
        "@babel/preset-react": "^7.7.2",
        "@gooddata/catalog-export": "9.2.0",
        "@gooddata/sdk-ui-kit": "9.2.0",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.1",
        "babel-loader": "^8.0.5",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "css-loader": "^6.7.1",
        "dotenv": "16.0.3",
        "dotenv-webpack": "^8.0.1",
        "eslint": "^8.51.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-import-esm": "^1.2.1",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "html-webpack-plugin": "^5.3.1",
        "husky": "^8.0.0",
        "lint-staged": "^15.0.2",
        "process": "^0.11.10",
        "prettier": "^3.0.3",
        "source-map-loader": "^4.0.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^8.3.0",
        "typescript": "5.0.2",
        "util": "^0.12.3",
        "webpack": "^5.58.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.1"
    },
    "lint-staged": {
        "*.{js,ts,tsx,json,scss,md,yaml,html}": "prettier --write",
